{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "OnlineExamination": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/OnlineExamination",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": [
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "OnlineExamination:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "OnlineExamination:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "OnlineExamination:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/assets/exam-admin/css",
              "src/assets/exam-admin/js",
              "src/assets/exam-admin/js/core",
              "src/assets/exam-admin/js/plugins",
              "src/assets/exam-admin/img",
              "src/assets/exam-admin/img/faces",
              "src/assets/exam-admin/img/flags"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": [
              "src/assets/exam-admin/js/core/jquery.min.js",
              "src/assets/exam-admin/js/core/popper.min.js",
              "src/assets/exam-admin/js/core/bootstrap-material-design.min.js",
              "src/assets/exam-admin/js/plugins/perfect-scrollbar.jquery.min.js",
              "src/assets/exam-admin/js/plugins/moment.min.js",
              "src/assets/exam-admin/js/plugins/sweetalert2.js",
              "src/assets/exam-admin/js/plugins/jquery.validate.min.js",
              "src/assets/exam-admin/js/plugins/jquery.bootstrap-wizard.js",
              "src/assets/exam-admin/js/plugins/bootstrap-selectpicker.js",
              "src/assets/exam-admin/js/plugins/bootstrap-datetimepicker.min.js",
              "src/assets/exam-admin/js/plugins/jquery.dataTables.min.js",
              "src/assets/exam-admin/js/plugins/bootstrap-tagsinput.js",
              "src/assets/exam-admin/js/plugins/jasny-bootstrap.min.js",
              "src/assets/exam-admin/js/plugins/fullcalendar.min.js",
              "src/assets/exam-admin/js/plugins/jquery-jvectormap.js",
              "src/assets/exam-admin/js/plugins/nouislider.min.js",
              "https://cdnjs.cloudflare.com/ajax/libs/core-js/2.4.1/core.js",
              "src/assets/exam-admin/js/plugins/arrive.min.js",
              "https://buttons.github.io/buttons.js",
              "src/assets/exam-admin/js/plugins/chartist.min.js",
              "src/assets/exam-admin/js/plugins/bootstrap-notify.js",
              "src/assets/exam-admin/js/tg-material.min.js?v=2.1.0",
              "src/assets/exam-admin/js/backend.js" 
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "OnlineExamination:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "OnlineExamination:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "OnlineExamination"
}